//   url      = env("DATABASE_URL")
// }


datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}


model UtxoRecord {
    id        Int      @id @default(autoincrement())
    used      DateTime
    usedById  String
    hash      String
    txHash    String
}

model WhitelistedUser {
    id        String      @id @unique
    claimed   Boolean
}
